    "@types/react-dom": "^15.5.1",
    "@types/serialize-javascript": "^1.3.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-relay": "^1.1.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "concurrently": "^3.5.0",
    "fs-extra": "^4.0.0",
